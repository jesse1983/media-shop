{
  "name": "media-shop-node-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/**/*.ts --fix",
    "build": "tsc -p .",
    "preseed": "prisma db push",
    "seed": "prisma db seed",
    "prestart": "prisma db push",
    "start": "NODE_ENV=production node dist/src/server.js",
    "dev": "nodemon src/server.ts",
    "pretest": "docker-compose up db-test -d",
    "pretest:watch": "docker-compose up db-test -d",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node src/config/prisma/seed.ts",
    "schema": "src/config/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-express-validator": "^1.0.0",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "prisma": "^4.13.0",
    "swagger-express-validator": "^1.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  }
